method = "leapBackward",
tuneGrid = data.frame(nvmax = 1:p),
trControl = train.control)
step.model$results
step.model$bestTune
summary(step.model$finalModel)
print(coef(step.model$finalModel, step.model$bestTune[1,]))
return(step.model)
}
