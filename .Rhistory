ecm <- function(modelo, a_testear) {
y.hat <- predict(modelo, a_testear)
return(
c(mean((y.hat - a_testear$G3)^2), summary(modelo)$r.squared))
}
ecm_pred <- function(predicciones, a_testear) {
return(mean((predicciones - a_testear$G3)^2))
}
